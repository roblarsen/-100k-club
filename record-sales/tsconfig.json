{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./dist",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  "types": ["webpack-dev-server"]
  },
  "include": ["scripts/**/*", "../core/formatCurrency.ts", "../core/dateFormatter.ts", "../core/getVenueName.ts"],
  "paths": ["node_modules/**/*"]
}