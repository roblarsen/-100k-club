{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./dist",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
  "types": ["webpack-dev-server"]
  },
  "include": ["scripts/**/*"],
  "paths": ["node_modules/**/*"]
}